		"prefix": ":silentlywrap",
		"body": [
			"<<silently>>\n\t$TM_SELECTED_TEXT\n<</silently>>"
		]
	},
	"Macro: type": {
		"prefix": ":type",
		"body": [
			"<<type ${1:speed} $2>>\n\t$0\n<</type>>"
		]
	},
