{
	"StoryData": {
		"prefix": "StoryData",
		"body": [
			":: StoryData",
			"{",
			"\t\"ifid\": \"$1\",",
			"\t\"format\": \"$2\",",
			"\t\"format-version\": \"$3\"",
			"}"
		],
		"description": "StoryData JSON chunk."
	},
	"Macro: link": {
		"prefix": ":link",
		"body": [
			"<<link \"${1:title}\" \"${2:passage}\">>$3<</link>>"
		]
	},
	"Macro: link (no passage)": {
		"prefix": ":linknp",
		"body": [
			"<<link \"${1:title}\">>$2<</link>>"
		]
	},
	"Macro: capture": {
		"prefix": ":capture",
		"body": [
			"<<capture ${1:variables}>>\n\t$2\n<</capture>>"
		]
	},
	"Macro: set": {
		"prefix": ":set",
		"body": [
			"<<set $1>>"
		]
	},
	"Macro: unset": {
		"prefix": ":unset",
		"body": [
			"<<unset ${1:variable}>>"
		]
	},
	"Macro: run": {
		"prefix": ":run",
		"body": [
			"<<run $1>>"
		]
	},
	"Macro: script": {
		"prefix": ":script",
		"body": [
			"<<script>>$1<</script>>"
		]
	},
	"Macro: =": {
		"prefix": ":=",
		"body": [
			"<<= $1>>"
		]
	},
	"Macro: -": {
		"prefix": ":-",
		"body": [
			"<<- $1>>"
		]
	},
	"Macro: include": {
		"prefix": ":include",
		"body": [
			"<<include \"${1:passage}\">>"
		]
	},
	"Macro: nobr": {
		"prefix": ":nobr",
		"body": [
			"<<nobr>>\n\t${1:contents}\n<</nobr>>"
		]
	},
	"Macro: nobr (wrap)": {
		"prefix": ":nobrwrap",
		"body": [
			"<<nobr>>\n\t$TM_SELECTED_TEXT\n<</nobr>>"
		]
	},
	"Macro: print": {
		"prefix": ":print",
		"body": [
